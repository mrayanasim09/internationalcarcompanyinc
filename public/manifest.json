{
  "name": "International Car Company Inc",
  "short_name": "ICC",
  "description": "Premium vehicles with transparent pricing and modern experience",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#1e90ff",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en-US",
  "categories": ["business", "automotive", "shopping"],
  "icons": [
    {
      "src": "/favicon.ico",
      "sizes": "any",
      "type": "image/x-icon"
    },
    {
      "src": "/prestige-auto-sales-logo.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/prestige-auto-sales-logo.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshot-mobile.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow"
    },
    {
      "src": "/screenshot-desktop.png",
      "sizes": "1920x1080",
      "type": "image/png",
      "form_factor": "wide"
    }
  ],
  "shortcuts": [
    {
      "name": "Browse Cars",
      "short_name": "Browse",
      "description": "Browse our car inventory",
      "url": "/browse",
      "icons": [
        {
      "src": "/prestige-auto-sales-logo.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Contact Us",
      "short_name": "Contact",
      "description": "Get in touch with us",
      "url": "/contact",
      "icons": [
        {
          "src": "/prestige-auto-sales-logo.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "About Us",
      "short_name": "About",
      "description": "Learn more about International Car Company Inc.",
      "url": "/about",
      "icons": [
        {
          "src": "/prestige-auto-sales-logo.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate",
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/pdf": [".pdf"],
        "image/*": [".jpg", ".jpeg", ".png", ".webp"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+internationalcarcompany",
      "url": "/?protocol=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "permissions": [
    "geolocation",
    "notifications"
  ],
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7"
} 