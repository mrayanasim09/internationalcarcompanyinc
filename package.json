{
  "name": "international-car-company-inc",
  "packageManager": "pnpm@10.0.0",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "analyze": "ANALYZE=true next build",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "lighthouse:ci": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "bundle:check": "pnpm build && node scripts/analyze-bundle.js",
    "optimize": "pnpm analyze && pnpm analyze:bundle",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf .next out dist",
    "clean:all": "rm -rf .next out dist node_modules pnpm-lock.yaml",
    "reinstall": "pnpm clean:all && pnpm install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "next lint --fix",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "performance:test": "pnpm build && pnpm lighthouse:ci",
    "performance:monitor": "pnpm dev & sleep 10 && pnpm lighthouse:ci && pkill -f 'next dev'"
  },
